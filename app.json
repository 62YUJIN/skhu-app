{
  "expo": {
    "name": "SKHU's",
    "description": "The Essential App for Sungkonghoe University students.",
    "slug": "skhus-app",
    "privacy": "public",
    "sdkVersion": "29.0.0",
    "platforms": ["ios", "android"],
    "version": "0.1.1",
    "orientation": "portrait",
    "icon": "./assets/imgs/icon.png",
    "primaryColor": "#ffffff",
    "assetBundlePatterns": [
      "assets/imgs/*"
    ],
    "ios": {
      "bundleIdentifier": "com.sowl.skhus",
      "buildNumber": "0.1.1",
      "icon": "./assets/imgs/ios.png",
      "supportsTablet": true
    },
    "android": {
      "versionCode": 1,
      "package": "com.sowl.skhus",
      "adaptiveIcon":{
        "foregroundImage":"./assets/imgs/androidAdaptive.png"
      }
    },
    "splash": {
      "image": "./assets/imgs/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ]
    
  }
}
