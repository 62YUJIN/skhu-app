{
  "expo": {
    "name": "SKHU's",
    "description": "This project is really great.",
    "slug": "skhus-app",
    "privacy": "public",
    "sdkVersion": "29.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "ios": {
      "bundleIdentifier": "com.sowl.skhus",
      "icon": "./assets/ios.png",
      "supportsTablet": true
    },
    "android": {
      "package": "com.sowl.skhus",
      "adaptiveIcon":{
        "foregroundImage":"./assets/androidAdaptive.png"
      }
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ]
    
  }
}
